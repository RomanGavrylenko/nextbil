{
  "name": "homework",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "apollo-client": "^2.6.10",
    "formik": "^2.2.6",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.8",
